{
  "name": "English Mate",
  "version": "1.0.1",
  "manifest_version": 3,
  "action": {
    "default_title": "Open English Mate"
  },
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "sidePanel"
  ]
}